            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/rectangulo"
                    android:layout_width="209dp"
                    android:layout_height="wrap_content"
                    android:layout_span="1"
                    android:text="Rectangulo" />

                <TextView
                    android:id="@+id/rectangulCant"
                    android:layout_width="202dp"
                    android:layout_height="wrap_content"
                    android:text="TextView" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/linea"
                    android:layout_width="209dp"
                    android:layout_height="wrap_content"
                    android:layout_span="1"
                    android:text="Linea" />

                <TextView
                    android:id="@+id/lineaCant"
                    android:layout_width="202dp"
                    android:layout_height="wrap_content"
                    android:text="TextView" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/poligono"
                    android:layout_width="209dp"
                    android:layout_height="match_parent"
                    android:layout_span="1"
                    android:text="Poligono" />

                <TextView
                    android:id="@+id/poligonoCant"
                    android:layout_width="202dp"
                    android:layout_height="wrap_content"
                    android:text="TextView" />

            </TableRow>

        </TableLayout>
    </ScrollView>

    <TextView
        android:id="@+id/principalText3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_gravity="center_horizontal"
        android:gravity="center_horizontal"
        android:text="REPORTE DE ANIMACIONES USADAS"
        android:textAlignment="center"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.498" />

    <ScrollView
        android:id="@+id/reporteAnimaciones"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/animacion"
                    android:layout_width="209dp"
                    android:layout_height="wrap_content"
                    android:layout_span="1"
                    android:text="ANIMACION"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/animacionCant"
                    android:layout_width="202dp"
                    android:layout_height="wrap_content"
                    android:text="CANTIDAD DE USOS"
                    android:textSize="15dp" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/lineaAnim"
                    android:layout_width="209dp"
                    android:layout_height="wrap_content"
                    android:layout_span="1"
                    android:text="Linea" />

                <TextView
                    android:id="@+id/lineaAnimCant"
                    android:layout_width="202dp"
